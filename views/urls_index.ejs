<h1>My URLs</h1>

<table>
  <thead>
    <tr>
      <th>Short URL</th>
      <th>Long URL</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% for (const id in urlDatabase) { %>
      <tr>
  <td><%= id %></td>
  <td><%= urlDatabase[id] %></td>
  <td>
    <a href="/urls/<%= id %>">Edit</a>
    <form method="POST" action="/urls/<%= id %>/delete" style="display:inline;">
      <input type="submit" value="Delete">
    </form>
  </td>
</tr>
    <% } %>
  </tbody>
</table>